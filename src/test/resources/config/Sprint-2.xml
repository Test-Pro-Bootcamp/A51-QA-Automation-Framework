<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sprint-2"  parallel="classes" thread-count="3" verbose="1">
    <listeners>
        <listener class-name="util.listeners.TestListener"/>
        <listener class-name="util.listeners.AnnotationTransformer"/>
    </listeners>

    <test name="favoritesAndSearch">
        <parameter name="baseURL" value="https://qa.koel.app/"/>
        <parameter name="searchString" value="dar"/>
        <parameter name="searchString2" value="a"/>
        <classes>
            <class name="testcases.AllSongsTests"/>
        </classes>
    </test>
    <test name="albumsTest">
        <parameter name="baseURL" value="https://qa.koel.app/"/>
        <classes>
            <class name="testcases.PlaylistsTests"/>
            <class name="testcases.AlbumsTests"/>
        </classes>
    </test>
</suite>